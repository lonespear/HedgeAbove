# HedgeAbove Environment Variables
# Copy this file to .env and fill in your API keys

# Market Data API Keys
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
POLYGON_API_KEY=your_polygon_key_here
FINNHUB_API_KEY=your_finnhub_key_here

# Database Configuration (if using PostgreSQL)
DATABASE_URL=postgresql://username:password@localhost:5432/hedgeabove

# Authentication (if implementing user accounts)
SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Email Service (for notifications/alerts)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password

# Stripe (for payment processing)
STRIPE_PUBLISHABLE_KEY=pk_test_xxx
STRIPE_SECRET_KEY=sk_test_xxx

# Feature Flags
ENABLE_REAL_TIME_DATA=false
ENABLE_ML_PREDICTIONS=true
ENABLE_OPTIONS_PRICING=true

# Logging
LOG_LEVEL=INFO
